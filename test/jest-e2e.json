{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@ittlr/nestjs-bootstrap/(.*)": "<rootDir>/../libs/nestjs-bootstrap/src/$1",
    "@ittlr/nestjs-bootstrap": "<rootDir>/../libs/nestjs-bootstrap/src"
  }
}